<h1><%= @genre.name.capitalize %></h1>

<h2>Artists</h2>
<ul>
  <% @genre.artists.each do | artist | %>
  <li>
    <a href="/artists/<%=artist.slug%>"><%=artist.name%></a>
  </li>
</ul>
<% end %>
<h2>Songs</h2>
<ul>
  <% @genre.songs.each do |songs| %>
        <li>
          <a href="/songs/<%=songs.slug%>"><%=songs.name%></a>
        </li>
    <% end %>
</ul>
